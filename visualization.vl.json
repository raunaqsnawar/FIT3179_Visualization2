{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 800,
  "height": 500,
  "projection": {"type": "equalEarth"},
  "vconcat": [
    {
      "layer": [
        {
          "title": {
            "text": "World Happiness Score Analysis with Social Support by Countries",
            "anchor": "middle"
          },
          "data": {
            "url": "https://raw.githubusercontent.com/KaneSec/vega_lite/main/2_symbol_map/js/ne_110m_admin_0_countries.topojson",
            "format": {
              "type": "topojson",
              "feature": "ne_110m_admin_0_countries"
            }
          },
          "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
        },
        {
          "data": {
            "url": "https://raw.githubusercontent.com/raunaqsnawar/FIT3179_Visualization2/main/data2019.csv"
          },
          "mark": {"type": "circle", "tooltip": {"content": "data"}},
          "encoding": {
            "longitude": {"field": "Longitude", "type": "quantitative"},
            "latitude": {"field": "Latitude", "type": "quantitative"},
            "size": {
              "field": "Social support",
              "type": "quantitative",
              "scale": {"domain": [0, 1.7]}
            },
            "color": {
              "field": "Score",
              "type": "quantitative",
              "scale": {"scheme": "greens"}
            },
            "tooltip": [
              {
                "field": "Country or region",
                "type": "nominal",
                "title": "Country or region"
              },
              {"field": "Score", "type": "quantitative"},
              {"field": "GDP per capita", "type": "quantitative"},
              {"field": "Social support", "type": "quantitative"},
              {"field": "Healthy life expectancy", "type": "quantitative"},
              {"field": "Freedom to make life choices", "type": "quantitative"},
              {"field": "Generosity", "type": "quantitative"},
              {"field": "Perceptions of corruption", "type": "quantitative"}
            ]
          }
        }
      ]
    },
    {
      "width": 800,
      "height": 600,
      "title": {
        "text": "World Happiness Score Analysis by Countries",
        "anchor": "middle"
      },
      "data": {
        "url": "https://raw.githubusercontent.com/KaneSec/vega_lite/main/3_choropleth_map/js/ne_110m_admin_0_countries.topojson",
        "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
      },
      "transform": [
        {
          "lookup": "properties.NAME",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/raunaqsnawar/FIT3179_Visualization2/main/data2019.csv"
            },
            "key": "Country or region",
            "fields": ["Score"]
          }
        },
        {"calculate": "datum.Score", "as": "Hapiness Score"}
      ],
      "projection": {"type": "equirectangular"},
      "mark": {"type": "geoshape"},
      "encoding": {
        "color": {
          "field": "Hapiness Score",
          "type": "quantitative",
          "scale": {"type": "log"}
        },
        "tooltip": [
          {
            "field": "Country or region",
            "type": "nominal",
            "title": "Country or region"
          },
          {"field": "Score", "type": "quantitative"}
        ]
      }
    },
    {
      "hconcat": [
        {
          "width": 230,
          "height": 195,
          "title": {
            "text": "Perceptions of corruption Vs Happiness Score",
            "anchor": "middle"
          },
          "data": {
            "url": "https://raw.githubusercontent.com/raunaqsnawar/FIT3179_Visualization2/main/data2019.csv"
          },
          "mark": "point",
          "encoding": {
            "x": {"bin": true, "field": "Score"},
            "y": {"field": "Perceptions of corruption", "type": "quantitative"},
            "color": {"value": "#279c2b"},
            "tooltip": [
              {
                "field": "Country or region",
                "type": "nominal",
                "title": "Country or region"
              },
              {"field": "Score", "type": "quantitative"},
              {"field": "GDP per capita", "type": "quantitative"},
              {"field": "Social support", "type": "quantitative"},
              {"field": "Healthy life expectancy", "type": "quantitative"},
              {"field": "Freedom to make life choices", "type": "quantitative"},
              {"field": "Generosity", "type": "quantitative"},
              {"field": "Perceptions of corruption", "type": "quantitative"}
            ]
          }
        },
        {
          "width": 230,
          "title": {
            "text": "Healthy life expectancy Vs Happiness Score",
            "anchor": "middle"
          },
          "data": {
            "url": "https://raw.githubusercontent.com/raunaqsnawar/FIT3179_Visualization2/main/data2019.csv"
          },
          "mark": {"type": "point"},
          "encoding": {
            "x": {"bin": true, "field": "Score"},
            "y": {"field": "Healthy life expectancy", "type": "quantitative"},
            "color": {"value": "#279c2b"},
            "tooltip": [
              {
                "field": "Country or region",
                "type": "nominal",
                "title": "Country or region"
              },
              {"field": "Score", "type": "quantitative"},
              {"field": "GDP per capita", "type": "quantitative"},
              {"field": "Social support", "type": "quantitative"},
              {"field": "Healthy life expectancy", "type": "quantitative"},
              {"field": "Freedom to make life choices", "type": "quantitative"},
              {"field": "Generosity", "type": "quantitative"},
              {"field": "Perceptions of corruption", "type": "quantitative"}
            ]
          }
        },
        {
          "width": 230,
          "title": {"text": "Freedom Vs Happiness Score", "anchor": "middle"},
          "data": {
            "url": "https://raw.githubusercontent.com/raunaqsnawar/FIT3179_Visualization2/main/data2019.csv"
          },
          "mark": {"type": "bar", "tooltip": {"content": "data"}},
          "encoding": {
            "x": {"bin": true, "field": "Score"},
            "y": {
              "field": "Freedom to make life choices",
              "type": "quantitative"
            },
            "color": {"value": "#279c2b"},
            "tooltip": [
              {
                "field": "Country or region",
                "type": "nominal",
                "title": "Country or region"
              },
              {"field": "Score", "type": "quantitative"},
              {"field": "GDP per capita", "type": "quantitative"},
              {"field": "Social support", "type": "quantitative"},
              {"field": "Healthy life expectancy", "type": "quantitative"},
              {"field": "Freedom to make life choices", "type": "quantitative"},
              {"field": "Generosity", "type": "quantitative"},
              {"field": "Perceptions of corruption", "type": "quantitative"}
            ]
          }
        }
      ]
    },
    {
      "title": {
        "text": "Matrix plot analysis for Generosity, GDP per capita, and Social support with Perceptions of corruption.",
        "anchor": "middle"
      },
      "repeat": {
        "row": ["Generosity", "GDP per capita", "Social support"],
        "column": ["Social support", "GDP per capita", "Generosity"]
      },
      "spec": {
        "data": {
          "url": "https://raw.githubusercontent.com/raunaqsnawar/FIT3179_Visualization2/main/data2019.csv"
        },
        "mark": "point",
        "selection": {
          "brush": {
            "type": "interval",
            "resolve": "union",
            "on": "[mousedown[event.shiftKey], window:mouseup] > window:mousemove!",
            "translate": "[mousedown[event.shiftKey], window:mouseup] > window:mousemove!",
            "zoom": "wheel![event.shiftKey]"
          },
          "grid": {
            "type": "interval",
            "resolve": "global",
            "bind": "scales",
            "translate": "[mousedown[!event.shiftKey], window:mouseup] > window:mousemove!",
            "zoom": "wheel![!event.shiftKey]"
          }
        },
        "encoding": {
          "x": {"field": {"repeat": "column"}, "type": "quantitative"},
          "y": {
            "field": {"repeat": "row"},
            "type": "quantitative",
            "axis": {"minExtent": 30}
          },
          "color": {
            "condition": {
              "bin": true,
              "selection": "brush",
              "field": "Perceptions of corruption",
              "type": "quantitative"
            },
            "value": "grey"
          },
          "tooltip": [
            {
              "field": "Country or region",
              "type": "nominal",
              "title": "Country or region"
            },
            {"field": "Score", "type": "quantitative"},
            {"field": "GDP per capita", "type": "quantitative"},
            {"field": "Social support", "type": "quantitative"},
            {"field": "Healthy life expectancy", "type": "quantitative"},
            {"field": "Freedom to make life choices", "type": "quantitative"},
            {"field": "Generosity", "type": "quantitative"},
            {"field": "Perceptions of corruption", "type": "quantitative"}
          ]
        }
      }
    }
  ]
}